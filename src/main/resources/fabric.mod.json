{
  "schemaVersion": 1,
  "id": "rainimator",
  "version": "${version}",
  "name": "Rainimator",
  "description": "Rainimator Minecraft Animation mod. @Rainimator @Rainolaguer",
  "authors": [
    "Triceratops",
    "IAFEnvoy"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/rainimatormod",
    "sources": "https://github.com/RainimatorModDev/RainimatorMod"
  },
  "license": "LGPL-3.0",
  "icon": "logo.jpg",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.rainimator.rainimatormod.RainimatorMod"
    ],
    "client": [
      "com.rainimator.rainimatormod.RainimatorModClient"
    ],
    "cardinal-components-entity": [
      "com.rainimator.rainimatormod.network.ManaComponentEntry"
    ],
    "emi": [
      "com.rainimator.rainimatormod.compat.emi.BossSpawnRecipePlugin"
