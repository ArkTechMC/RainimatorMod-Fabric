{
  "schemaVersion": 1,
  "id": "rainimator",
  "version": "${version}",
  "name": "Rainimator Mod",
  "description": "Rainimator Minecraft Animation mod. @Rainimator @Rainolaguer",
  "authors": [
    "Triceratops",
    {
      "name": "IAFEnvoy",
      "contact": {
        "homepage": "https://iafenvoy.com",
        "sources": "https://github.com/IAFEnvoy"
      }
    }
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/rainimatormod",
    "sources": "https://github.com/RainimatorModDev/RainimatorMod",
    "issues": "https://github.com/RainimatorModDev/RainimatorMod/issues"
  },
  "license": "LGPL-3.0",
  "icon": "logo.jpg",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.rainimator.mod.RainimatorMod"
    ],
    "client": [
      "dev.rainimator.mod.RainimatorModClient"
    ],
    "cardinal-components-entity": [
      "dev.rainimator.mod.data.component.ModComponentEntry"
    ],
    "emi": [
      "dev.rainimator.mod.compat.emi.BossSpawnRecipePlugin"
    ],
    "annotation_lib": [
      "dev.rainimator.mod.data.config.ModConfig",
      "dev.rainimator.mod.registry.RainimatorBlockEntities",
      "dev.rainimator.mod.registry.RainimatorBlocks",
      "dev.rainimator.mod.registry.RainimatorBanners",
      "dev.rainimator.mod.registry.RainimatorEffects",
      "dev.rainimator.mod.registry.RainimatorEnchantments",
      "dev.rainimator.mod.registry.RainimatorEntities",
      "dev.rainimator.mod.registry.RainimatorItems",
      "dev.rainimator.mod.registry.RainimatorItemGroups",
      "dev.rainimator.mod.registry.RainimatorPotions",
      "dev.rainimator.mod.registry.RainimatorSkulls",
      "dev.rainimator.mod.network.EnderBookActionHandler"
    ],
    "annotation_lib_client": [
      "dev.rainimator.mod.network.EnderBookOpenHandler",
      "dev.rainimator.mod.registry.RainimatorKeybindings"
    ],
    "annotation_lib_server": [
    ]
  },
  "custom": {
    "cardinal-components": [
      "rainimator:mana",
      "rainimator:ability"
    ],
    "avaritia-disable-recipe": [
      "rainimator:ender_big_sword"
    ]
  },
  "accessWidener": "rainimator.accesswidener",
  "mixins": [
    "rainimator.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.0",
    "fabric": "*",
    "minecraft": "1.20.1",
    "annotation_lib": "*",
    "mcrconvertlib": "*",
    "trinkets": "*"
  }
}
