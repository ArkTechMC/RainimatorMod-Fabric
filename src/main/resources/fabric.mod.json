      "rainimator:mana"
    ]
  },
  "accessWidener": "rainimator.accesswidener",
  "mixins": [
    "rainimator.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.0",
    "fabric": "*",
    "minecraft": "1.20.1",
    "annotation_lib": "*",
    "mcrconvertlib": "*",
    "trinkets": "*"
