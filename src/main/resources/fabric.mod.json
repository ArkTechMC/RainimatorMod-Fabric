{
  "schemaVersion": 1,
  "id": "rainimator",
  "version": "${version}",
  "name": "Rainimator",
  "description": "Rainimator Minecraft Animation mod. @Rainimator @Rainolaguer",
  "authors": [
    "Triceratops",
    "IAFEnvoy"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/rainimatormod",
    "sources": "https://github.com/RainimatorModDev/RainimatorMod"
  },
  "license": "LGPL-3.0",
  "icon": "logo.jpg",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.rainimator.mod.RainimatorMod"
    ],
    "client": [
      "dev.rainimator.mod.RainimatorModClient"
    ],
    "cardinal-components-entity": [
      "dev.rainimator.mod.network.ManaComponentEntry"
    ],
    "emi": [
      "dev.rainimator.mod.compat.emi.BossSpawnRecipePlugin"
    ],
    "annotation_lib": [
      "dev.rainimator.mod.data.config.ModConfig",
      "dev.rainimator.mod.registry.ModBlockEntities",
      "dev.rainimator.mod.registry.ModBlocks",
      "dev.rainimator.mod.registry.ModBanners",
      "dev.rainimator.mod.registry.ModEffects",
      "dev.rainimator.mod.registry.ModEnchantments",
      "dev.rainimator.mod.registry.ModEntities",
      "dev.rainimator.mod.registry.ModItems",
      "dev.rainimator.mod.registry.ModItemGroups",
      "dev.rainimator.mod.registry.ModParticles",
      "dev.rainimator.mod.registry.ModPotions",
      "dev.rainimator.mod.registry.ModSkulls",
      "dev.rainimator.mod.network.EnderBookActionHandler",
      "dev.rainimator.mod.network.AbilityHotkeyHandler"
    ],
    "annotation_lib_client": [
      "dev.rainimator.mod.network.EnderBookOpenHandler",
      "dev.rainimator.mod.registry.ModKeybindings"
    ],
    "annotation_lib_server": [
    ]
  },
  "custom": {
    "cardinal-components": [
      "rainimator:mana"
    ]
  },
  "accessWidener": "rainimator.accesswidener",
  "mixins": [
    "rainimator.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.0",
    "fabric": "*",
    "minecraft": "1.20.1",
    "annotation_lib": "*",
    "mcrconvertlib": "*",
    "trinkets": "*"
  }
}
